{
  "name": "vuepressify",
  "version": "0.0.0",
  "description": "Vuetify theme for Vuepress",
  "main": "n/a",
  "scripts": {
    "lint": "eslint --ext .js,.vue --fix docs && markdownlint docs/*.md *.md && prettier --list-different --write \"**/*.{js,json,css,vue,md}\"",
    "start": "vuepress dev docs",
    "build": "vuepress build docs",
    "test": "echo \"Error: no test specified\" && exit 1"
